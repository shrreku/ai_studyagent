  "keyConcepts": [
    {
      "concept": "Boundary layer development",
      "explanation": "The boundary layer is the thin region near a surface where velocity, temperature, and concentration gradients are significant. Includes velocity, thermal, and concentration boundary layers."
    },
    {
      "concept": "Local vs. average convection coefficients",
      "explanation": "Local coefficients describe conditions at a specific point, while average coefficients describe overall conditions over a surface."
    },
    {
      "concept": "Dimensionless parameters",
      "explanation": "Key dimensionless groups that characterize convective transport: Reynolds (Re), Prandtl (Pr), Schmidt (Sc), Nusselt (Nu), Sherwood (Sh)."
    },
    {
      "concept": "Boundary layer analogies",
      "explanation": "Mathematical similarities between momentum, heat, and mass transfer that allow predictions of one transport mode from another."
    }
  ],
