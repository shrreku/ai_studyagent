  "keyFormulas": [
    {
      "formula_name": "Nusselt Number",
      "formula": "Nu = hL/k",
      "description": "Dimensionless parameter relating convective to conductive heat transfer",
      "usage_context": "Used to calculate convective heat transfer coefficients in forced convection problems",
      "variables": null,
      "examples": null
    },
    {
      "formula_name": "Sherwood Number",
      "formula": "Sh = hmL/DAB",
      "description": "Dimensionless parameter relating convective to diffusive mass transfer",
      "usage_context": "Used in mass transfer calculations analogous to the Nusselt number in heat transfer",
      "variables": null,
      "examples": null
    },
    {
      "formula_name": "Reynolds Analogy",
      "formula": "St = Cf/2",
      "description": "Relates momentum transfer to heat transfer in turbulent flow",
      "usage_context": "Approximate relationship useful for estimating heat transfer coefficients from friction factors",
      "variables": null,
      "examples": null
    },
    {
      "formula_name": "Chilton-Colburn Analogy",
      "formula": "St Pr^{2/3} = Cf/2",
      "description": "Modified Reynolds analogy accounting for Prandtl number effects",
      "usage_context": "More accurate than Reynolds analogy for fluids with Pr \u2260 1",
      "variables": null,
      "examples": null
    },
    {
      "formula_name": "Prandtl Number",
      "formula": "Pr = \u03bd/\u03b1",
      "description": "Ratio of momentum diffusivity to thermal diffusivity",
      "usage_context": "Characterizes relative thickness of velocity and thermal boundary layers",
      "variables": null,
      "examples": null
    },
    {
      "formula_name": "Schmidt Number",
      "formula": "Sc = \u03bd/DAB",
      "description": "Ratio of momentum diffusivity to mass diffusivity",
      "usage_context": "Characterizes relative thickness of velocity and concentration boundary layers",
      "variables": null,
      "examples": null
    },
    {
      "formula_name": "Local Heat Transfer Coefficient",
      "formula": "hx = -k(\u2202T/\u2202y)|y=0 / (Ts - T\u221e)",
      "description": "Defines heat transfer at a specific point on a surface",
      "usage_context": "Used in boundary layer analysis for laminar flow over flat plates",
      "variables": null,
      "examples": null
    },
    {
      "formula_name": "Boundary Layer Thickness (Laminar)",
      "formula": "\u03b4 \u2248 5x/\u221a(Rex)",
      "description": "Approximate thickness of velocity boundary layer",
      "usage_context": "Estimating boundary layer development in laminar flow over flat plates",
      "variables": null,
      "examples": null
    }
  ]
