  "keyFormulas": [
    {
      "formula_name": "Reynolds Number",
      "formula": "Re = (\u03c1 * u * L)/\u03bc",
      "description": "Dimensionless number representing the ratio of inertial forces to viscous forces",
      "usage_context": "Used to predict flow regimes (laminar vs. turbulent) and characterize boundary layer development",
      "variables": {
        "\u03c1": "fluid density",
        "u": "flow velocity",
        "L": "characteristic length",
        "\u03bc": "dynamic viscosity"
      },
      "examples": [
        "Determining transition from laminar to turbulent flow (Re \u2248 5\u00d710^5 for flat plate)",
        "Scaling flow conditions between different systems"
      ]
    },
    {
      "formula_name": "Prandtl Number",
      "formula": "Pr = \u03bd/\u03b1 = (\u03bc * cp)/k",
      "description": "Dimensionless number representing the ratio of momentum diffusivity to thermal diffusivity",
      "usage_context": "Used to characterize relative thickness of velocity and thermal boundary layers",
      "variables": {
        "\u03bd": "kinematic viscosity",
        "\u03b1": "thermal diffusivity",
        "\u03bc": "dynamic viscosity",
        "cp": "specific heat at constant pressure",
        "k": "thermal conductivity"
      },
      "examples": [
        "Comparing boundary layer development in different fluids",
        "Determining appropriate heat transfer correlations"
      ]
    },
    {
      "formula_name": "Nusselt Number",
      "formula": "Nu = (h * L)/k",
      "description": "Dimensionless number representing the ratio of convective to conductive heat transfer",
      "usage_context": "Used to calculate convection heat transfer coefficients and characterize heat transfer efficiency",
      "variables": {
        "h": "convective heat transfer coefficient",
        "L": "characteristic length",
        "k": "thermal conductivity"
      },
      "examples": [
        "Calculating heat transfer rates from flat plates",
        "Designing heat exchangers"
      ]
    },
    {
      "formula_name": "Sherwood Number",
      "formula": "Sh = (hm * L)/D",
      "description": "Dimensionless number representing the ratio of convective to diffusive mass transfer",
      "usage_context": "Used in mass transfer problems analogous to Nusselt number in heat transfer",
      "variables": {
        "hm": "convective mass transfer coefficient",
        "L": "characteristic length",
        "D": "mass diffusivity"
      },
      "examples": [
        "Analyzing evaporation processes",
        "Designing mass transfer equipment"
      ]
    },
    {
      "formula_name": "Reynolds Analogy",
      "formula": "St = Cf/2",
      "description": "Relates skin friction coefficient to Stanton number for momentum and heat transfer",
      "usage_context": "Used when Pr \u2248 1 to estimate heat transfer from momentum transfer data",
      "variables": {
        "St": "Stanton number (Nu/(Re * Pr))",
        "Cf": "skin friction coefficient"
      },
      "examples": [
        "Estimating heat transfer coefficients from known friction factors",
        "Designing cooling systems for turbulent flow"
      ]
    }
  ]
}