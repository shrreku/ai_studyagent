  "key_formulas": [
    {
      "name": "Fourier's Law",
      "formula": "q = -kA(dT/dx)",
      "description": "Heat conduction rate through a material",
      "usage_context": "Calculating heat transfer through walls",
      "variables": {
        "q": "Heat transfer rate",
        "k": "Thermal conductivity",
        "A": "Cross-sectional area",
        "dT/dx": "Temperature gradient"
      },
      "examples": [
        "Calculating heat loss through a wall",
        "Determining insulation requirements"
      ]
    },
    {
      "name": "Newton's Law of Cooling",
      "formula": "q = hA(T_s - T_inf)",
      "description": "Convective heat transfer rate",
      "usage_context": "Cooling of hot surfaces",
      "variables": {
        "q": "Heat transfer rate",
        "h": "Convective heat transfer coefficient",
        "A": "Surface area",
        "T_s": "Surface temperature",
        "T_inf": "Fluid temperature"
      },
      "examples": [
        "Calculating cooling rate of a hot plate",
        "Designing cooling systems"
      ]
    },
    {
      "name": "Stefan-Boltzmann Law",
      "formula": "E = \u03b5\u03c3T^4",
      "description": "Radiative heat emission",
      "usage_context": "Heat transfer from stars",
      "variables": {
        "E": "Radiative heat flux",
        "\u03b5": "Emissivity",
        "\u03c3": "Stefan-Boltzmann constant",
        "T": "Absolute temperature"
      },
      "examples": [
        "Calculating solar radiation",
        "Designing thermal protection systems"
      ]
    }
  ],
