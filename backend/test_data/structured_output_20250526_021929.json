  "keyFormulas": [
    {
      "formula_name": "Fourier's Law",
      "formula": "q = -k * A * (dT/dx)",
      "description": "Calculates heat flux in conduction",
      "usage_context": "Heat loss through walls and insulation materials",
      "variables": {
        "q": "Heat flux (W/m\u00b2)",
        "k": "Thermal conductivity (W/m\u00b7K)",
        "A": "Cross-sectional area (m\u00b2)",
        "dT/dx": "Temperature gradient (K/m)"
      },
      "examples": [
        "Calculating heat loss through a brick wall",
        "Determining insulation requirements for a pipe"
      ]
    },
    {
      "formula_name": "Newton's Law of Cooling",
      "formula": "q = h * A * (T_s - T_inf)",
      "description": "Calculates convective heat transfer",
      "usage_context": "Cooling of surfaces by fluids",
      "variables": {
        "q": "Heat transfer rate (W)",
        "h": "Convective heat transfer coefficient (W/m\u00b2\u00b7K)",
        "A": "Surface area (m\u00b2)",
        "T_s": "Surface temperature (K)",
        "T_inf": "Fluid temperature (K)"
      },
      "examples": [
        "Cooling of electronic components",
        "Heat transfer in heat exchangers"
      ]
    },
    {
      "formula_name": "Stefan-Boltzmann Law",
      "formula": "E = \u03b5\u03c3T^4",
      "description": "Calculates radiant heat flux",
      "usage_context": "Radiation from surfaces",
      "variables": {
        "E": "Radiant heat flux (W/m\u00b2)",
        "\u03b5": "Emissivity",
        "\u03c3": "Stefan-Boltzmann constant (5.67\u00d710\u207b\u2078 W/m\u00b2\u00b7K\u2074)",
        "T": "Absolute temperature (K)"
      },
      "examples": [
        "Solar panel heating",
        "Radiative cooling of buildings"
      ]
    }
  ]
