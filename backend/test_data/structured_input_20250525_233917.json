{
  "overall_goal": "Master the fundamental principles of heat transfer, including conduction, convection, and radiation, and apply them to solve practical problems",
  "total_study_day": 5,
  "hour_per_day": 4.0,
  "core_concepts": [
    {
      "name": "Conduction",
      "explanation": "Heat transfer through a solid or stationary fluid due to temperature differences, governed by Fourier's Law",
      "importance": "high",
      "related_concepts": [
        "Thermal conductivity",
        "Steady-state heat transfer"
      ],
      "examples": [
        "Heat flow through a metal rod",
        "Insulation in buildings"
      ],
      "difficulty_level": "basic"
    },
    {
      "name": "Convection",
      "explanation": "Heat transfer between a surface and a moving fluid, described by Newton's Law of Cooling",
      "importance": "high",
      "related_concepts": [
        "Forced convection",
        "Natural convection"
      ],
      "examples": [
        "Cooling of electronics with fans",
        "Ocean currents affecting climate"
      ],
      "difficulty_level": "intermediate"
    },
    {
      "name": "Radiation",
      "explanation": "Heat transfer via electromagnetic waves, following the Stefan-Boltzmann Law",
      "importance": "medium",
      "related_concepts": [
        "Blackbody radiation",
        "Emissivity"
      ],
      "examples": [
        "Solar energy absorption",
        "Thermal imaging"
      ],
      "difficulty_level": "intermediate"
    }
  ],
